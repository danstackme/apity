{
  "name": "@danstack/apity",
  "version": "0.1.0",
  "description": "Type-safe API client generator for React applications with file-based routing and runtime validation",
  "author": "Daniel Osmond",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danstackme/apity.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "apity-import": "./dist/cli/import-openapi.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "tsx scripts/generate.ts && vitest --typecheck.only",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run build",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.cjs",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.0.0",
    "react": "^18.0.0",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "openapi-types": "^12.1.3",
    "swagger2openapi": "^7.0.8",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@tanstack/react-query": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "axios": "^1.6.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.9",
    "zod": "^3.22.0"
  },
  "keywords": [
    "react",
    "typescript",
    "api",
    "client",
    "openapi",
    "swagger",
    "zod",
    "react-query",
    "file-based-routing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
