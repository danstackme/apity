{
  "name": "@danstackme/apity",
  "version": "0.5.9",
  "description": "Type-safe API client generator for React applications with file-based routing and runtime validation",
  "author": "Daniel Osmond",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danstackme/apity.git"
  },
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.cjs",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "apity-import": "./dist/scripts/import-openapi.js"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run --coverage",
    "test:coverage": "vitest run --coverage.enabled=true --coverage.reportOnFailure --coverage.reporter=text",
    "test:watch": "vitest",
    "test:types": "tsx scripts/generate.ts && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run build",
    "size": "size-limit",
    "release": "npm run lint && tsx scripts/release.ts",
    "prepare": "husky || true && effect-language-service patch",
    "test:bump": "vitest run --coverage.enabled=true --coverage.reportOnFailure --coverage.provider=v8 --coverage.reporter=text",
    "watch": "tsdown --watch"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "size-limit": [
    {
      "path": "dist/src/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/src/index.cjs",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.12.0",
    "react": "^19.0.0",
    "vite": "^6.2.2",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@effect/cli": "^0.73.2",
    "@tanstack/react-query": "^5.90.21",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "effect": "^3.19.16",
    "fs-extra": "^11.3.3",
    "glob": "^10.5.0",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "7.13.0",
    "openapi-typescript-codegen": "^0.27.0",
    "swagger2openapi": "^7.0.8",
    "tsx": "^4.21.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@effect/language-service": "^0.73.1",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.19.33",
    "@types/react": "^18.3.28",
    "@types/swagger2openapi": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.1",
    "rimraf": "^5.0.10",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4",
    "axios": "^1.12.0"
  },
  "keywords": [
    "react",
    "typescript",
    "api",
    "client",
    "openapi",
    "swagger",
    "zod",
    "react-query",
    "file-based-routing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "esbuild": "0.25.1",
    "axios": "^1.12.0",
    "form-data": "^4.0.4",
    "glob": "^10.5.0",
    "brace-expansion": "^1.1.12",
    "js-yaml": "^4.1.1"
  }
}
