{
  "name": "@danstack/apity",
  "version": "0.1.0",
  "description": "Type-safe API client generator with file-based routing and runtime validation",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsx scripts/generate.ts && vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "tsx scripts/generate.ts && vitest --typecheck.only",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "generate": "tsx scripts/generate.ts"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.0.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0",
    "glob": "^10.3.10",
    "tsx": "^4.7.0",
    "jsdom": "^24.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
